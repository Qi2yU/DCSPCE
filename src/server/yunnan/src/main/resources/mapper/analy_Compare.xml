<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.yunnan.mapper.analy_Compare_mapper">
    <resultMap id="data" type="CompareEntity">
        <result column="last_people_num" property="last_num"></result>
        <result column="now_people_num" property="now_num"></result>
        <result column="company_name" property="name"></result>
    </resultMap>
    <select id="get_data" resultMap="data">
        select t.last_people_num, t.now_people_num, c.company_name from ${timeTableId} as t join company_info as c
        where t.user_id = c.user_id
        and c.city = "${city}" and c.company_character = "${character}" and c.company_industry = "${industry}"
    </select>

    <select id="get_mounted_city" resultType="String">
        select distinct city from company_info
    </select>


    <select id="get_mounted_char" resultType="String">
        select distinct company_character from company_info
    </select>

    <select id="get_mounted_indu" resultType="String">
        select distinct company_industry from company_info
    </select>


</mapper>