<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.yunnan.mapper.analy_Sample">
        <resultMap id="Sample" type="java.util.Map">
            <result column="city" property="city_name" javaType="String"></result>
            <result column="count(*)" property="company_num" javaType="String"></result>
        </resultMap>

        <select id="get_data" resultMap="Sample">
            select city,count(*)from company_info group by city
        </select>

    

</mapper>