<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.yunnan.mapper.Refused_info_mapper">
    <resultMap id="empMap" type="com.example.yunnan.entity.empInfoRefused">
        <result column="modify_info" property="info"/>
    </resultMap>

    <select id="getEmpInfoRefused" resultMap="empMap">
        select
        d.modify_info
        from ${tableName} d
        <where>
            d.user_id = #{userId}
        </where>
    </select>

    <resultMap id="comMap" type="com.example.yunnan.entity.comInfoRefused">
        <result column="backed_info" property="info"/>
    </resultMap>

    <select id="getComInfoRefused" resultMap="comMap">
        select
        d.backed_info
        from company_info d
        <where>
            d.user_id = #{userId}
        </where>
    </select>

</mapper>